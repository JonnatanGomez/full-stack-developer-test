<!-- ##############  MODALS ################### -->
<!-- Crear -->
<div class="modal" id="msjModalNew" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">El vehiculo no existe, ¿Desea Crearlo?</h5>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form class="form-group" id="ticketFrm" method="POST">
          <div class="form-group mx-sm-3 mb-2">
            <p>Número de Placa</p>
            <input
              type="text"
              class="form-control form-control-lg"
              id="numberCar2"
              placeholder="NUMERO DE PLACA"
              name="number"
              minlength="3"
              id="number"
            />
          </div>
          <p>Tipo de Vehiculo</p>
          <div
            class="btn-group"
            role="group"
            aria-label="Basic"
            id="btnsTypes"
          ></div>
          <hr />
          <center>
            <button
              type="button"
              class="btn btn-success btn-lg mb-2"
              onclick="saveVehicle()"
            >
              Crear
              <i class="fa fa-floppy-o"></i>
            </button>
          </center>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">
          No, Gracias
        </button>
      </div>
    </div>
  </div>
</div>
