<script type="application/javascript">
  function validateSession(data) {
    if (data.status == "tokenError" || sessionGlb == false) {
      closeSession();
    }
  }
  function closeSession() {
    sessionStorage.setItem("idPais", null);
    sessionStorage.setItem("token", null);
    sessionStorage.setItem("session", null);
    paisGlb = null;
    window.location.replace("./");
  }
  function goToMain() {
    window.location.replace("./");
  }
  function goToLogin() {
    window.location.replace("./login.ejs");
  }
  function msjGlb(title, body, type) {
    $("#messageTitle").html(title);
    $("#messageBody").html(body);
    $("#modalMsj").modal("show");
  }
  showMenu();
  function showMenu() {
    if (sessionGlb) {
      $("#btnCloseSession").show();
    }
  }
</script>
